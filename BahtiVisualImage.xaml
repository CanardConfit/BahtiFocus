    <coreconverters:BooleanToVisibilityCollapsedConverter x:Key="VisibilityConverter" />
    <coreconverters:InverseBooleanToVisibilityCollapsedConverter x:Key="InverseBooleanToVisibilityCollapsedConverter" />
